<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rensisi.ariz.ariz">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.CaseActivity" />
        <activity
            android:name=".ui.ArizActivity"
            android:label="@string/title_activity_ariz"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.ApplicateActivity"
            android:label="@string/title_activity_applicate"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.BrowseActivity"
            android:label="@string/title_activity_browse"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.FavorateActivity"
            android:label="@string/title_activity_favorate"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ui.UserActivity"
            android:label="@string/title_activity_user"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".activity.FavoriteCaseActivity"></activity>
    </application>

</manifest>